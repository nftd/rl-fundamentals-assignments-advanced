  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "name": "Frozen Lake Investigation 1 - Solve",
      "type": "debugpy",
      "request": "launch",
      "module": "exercises.mdps.frozen_lake_investigation_1",
      "args": ["solve"],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    }
  ]
}
